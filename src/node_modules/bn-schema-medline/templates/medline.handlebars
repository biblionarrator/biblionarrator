<article><header>
<div><a class="medline_journal" title="{{record.Article.Journal.Title}}">{{record.Article.Journal.ISOAbbreviation}}.</a>
{{#if record.Article.Journal.JournalIssue.Volume}}<span class="medline_volume">{{record.Article.Journal.JournalIssue.Volume}}</span>{{/if}}
{{#if record.Article.Journal.JournalIssue.Issue}}.<span class="medline_issue">{{record.Article.Journal.JournalIssue.Issue}}</span>{{/if}}
{{#if record.Article.Journal.JournalIssue.PubDate}}
    {{#if record.Article.Journal.JournalIssue.PubDate.Year}}<span class="medline_year">{{record.Article.Journal.JournalIssue.PubDate.Year}}</span>{{/if}}
    {{#if record.Article.Journal.JournalIssue.PubDate.Month}}<span class="medline_month">{{record.Article.Journal.JournalIssue.PubDate.Month}}</span>{{/if}}
    {{#if record.Article.Journal.JournalIssue.PubDate.Day}}<span class="medline_day">{{record.Article.Journal.JournalIssue.PubDate.Day}}</span>{{/if}}
    {{#if record.Article.Journal.JournalIssue.PubDate.Season}}<span class="medline_season">{{record.Article.Journal.JournalIssue.PubDate.Season}}</span>{{/if}}
{{/if}}
</div>
<div><span class="medline_title">{{record.Article.ArticleTitle}}</span></div>
<div>
    {{#each record.Article.AuthorList.Author}}
        <a class="medline_author">{{LastName}} {{ForeName}}</a>
    {{/each}}
</div>
</header>
<section>
<div>
<span class="medline_abstract">{{record.Article.Abstract.AbstractText}}</span>
</div>
</section>
</article>
