<article><header>
{{#marc record}}
<div><span class="isbd_titleproper">{{f245.a}}</span> <span class="isbd_titleremainder">{{subfstring f245 'hbfgknpsc'}}</span></div>
{{#iffield '[17](00|10|11)'}}<div>{{#tr 'label.authors'}}Authors:{{/tr}} {{#field '[17](00|10|11)'}}{{#if index}}, {{/if}}<a href="/search?anchor={{record.id}}&amp;q=author_e:%22{{subfstring this 'abcdfghijklmnopqrstu'}}%22" class="isbd_author">{{subfstring this 'abcdefghijklmnopqrstu'}}</a>{{/field}}</div>{{/iffield}}
{{#field f250}}<div><span class="label_isbd_edition">{{#tr 'label.edition'}}Edition:{{/tr}} </span><span class="isbd_edition">{{subfstring this 'ab'}}</span></div>{{/field}}
{{#field f773}}<div><span class="label_isbd_source">{{#tr 'label.source'}}Source:{{/tr}} </span><span class="isbd_source">{{subfstring this 'atg'}}</span></div>{{/field}}
{{#field f260}}<div><span class="label_isbd_publisher">{{#tr 'label.publisher'}}Publisher:{{/tr}} </span><span class="isbd_publisher">{{subfstring this 'abcg'}}</span></div>{{/field}}
{{#field f246}}<div><span class="label_isbd_titlevariant">{{#tr 'label.othertitle'}}Other title:{{/tr}} </span><span class="isbd_titlevariant">{{subfstring this 'ab'}}</span></div>{{/field}}
{{#field f242}}<div><span class="label_isbd_titletranslated">{{#tr 'label.translatedtitle'}}Translated title:{{/tr}} </span><span class="isbd_titletranslated">{{subfstring this 'abh'}}</span></div>{{/field}}
{{#field f856}}<div><span class="label_isbd_uri">{{#tr 'label.onlineaccess'}}Online access:{{/tr}} </span><span class="isbd_uri"><a href="{{u}}">{{subffirst this 'y3zu'}}</a></span></div>{{/field}}
<div>{{#if f952}}{{#field f952}}{{#if c}}<span class="isbd_location">{{c}}</span>{{/if}} <span class="isbd_collection">({{subfstring this '8'}})</span>{{#if o}}<span class="isbd_callnum">{{o}}</span>{{/if}}{{/field}}
{{else}}
{{#field f852}}<div class="isbd_location">{{subfstring this 'abchijklmnz'}}</div>{{/field}}
{{#field f980}}<div class="isbd_location">{{#tr 'label.locmark'}}Loc. mark{{/tr}} {{g}}, {{#tr 'label.document'}}Document{{/tr}} {{c}}</div>{{/field}}
{{/if}}</div>
</header>
<section>
{{#field f300}}<div><span class="isbd_pagination">{{a}}</span><span class="isbd_physdesc">{{b}}</span><span class="isbd_dimensions">{{c}}</span></div>{{/field}}
{{#field '5..'}}<div><span class="isbd_note">{{subfstring this 'abcdefghijklmnopqrstuvwxyz'}}</span></div>{{/field}}
</section>
{{#iffield '6..'}}<section><span>{{#tr 'label.subjects'}}Subjects:{{/tr}}</span><ul>{{#field '6..'}}<li><a href="/search?anchor={{record.id}}&amp;q=subject_e:%22{{subfstring this 'abcdefghijklmnopqrstuvwxyz'}}%22" class="isbd_subject">{{subfstring this 'abcdfghijklmnopqrstu'}}{{#ifsubf 'vwxyz'}}--{{subfstring this 'vwxyz' '--'}}{{/ifsubf}}</a></li>{{/field}}</ul>
</section>{{/iffield}}
{{#iffield '072'}}<section>
{{#field '072'}}<div class="isbd_subjectclass">{{subfstring this '2'}}: <a href="/search?anchor={{record.id}}&amp;q=subjectclass_e:%22{{x}}%22">{{x}}</a></div>{{/field}}
</section>{{/iffield}}
{{/marc}}
</article>
