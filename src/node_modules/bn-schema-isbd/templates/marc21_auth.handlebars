<article><header>
{{#marc record}}
{{#field '1..'}}<div><span class="isbd_heading">{{subfstring this 'abcdefghijklmnopqrstuvwxyz'}}</span></div>{{/field}}
</header>
<section>
{{#field '6..'}}<div><span class="isbd_note">{{subfstring this 'abcdefghijklmnopqrstuvwxyz'}}</span></div>{{/field}}
{{#iffield '856'}}<span>{{#tr 'label.relatedlinks'}}Related links:{{/tr}} </span><ul>{{#field '856'}}<li><a href="{{u}}">{{subffirst this 'y3zu'}}</a></li>{{/field}}</ul>{{/iffield}}
{{#iffield '5..'}}<span class="isbd_seealso_label">{{#tr 'label.seealso'}}See also:{{/tr}} </span><ul>
{{#field '5..'}}<li><a class="isbd_seealso" href="/search?q=authid:{{subffirst this '37'}}">{{subfstring this 'abcdefghijklmnopqrsuvxyz'}}</a></li>{{/field}}
</ul>
{{/iffield}}
{{#iffield '4..'}}<span class="isbd_usedfor_label">{{#tr 'label.usedfor'}}Used for:{{/tr}} </span><ul>
{{#field '4..'}}<li><span class="isbd_usedfor">{{subfstring this 'abcdefghijklmnopqrsuvxyz'}}</span></li>{{/field}}
</ul>
{{/iffield}}
</section>
{{/marc}}
</article>
